
<div class="weather-explore__title">
<h3>
  <span><b></b>Your trip </span> 
  <span> to {{travelCity}}</span>
</h3> 
<h2>
    <span>On the</span>
    <span>{{dateToLocal travelDate}}</span>
  </h2>

</div>
  <section class="weather">
      {{> weather}}
  </section>
   
    <section class="explore">
      {{#if explore}}
      <div class="explore__title">
        <h3>TASTE</h3>
      </div>
      <div class="explore__img-container">
      {{#each explore}}
        {{> exploreImg}}
      {{/each}}
      </div>
      {{else}}
      <div class="explore__img-container">
        <p class="explore__img-container--no-show">No Picture found for the chosen destination</p>
      </div>
      {{/if}}
    </section>
